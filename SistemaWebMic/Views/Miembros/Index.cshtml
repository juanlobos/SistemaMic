          @Html.DisplayFor(modelItem => item.Comuna)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.TCelular)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.TCasa)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Facebook)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Estado_Civil)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Nombre_Pareja)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Hijos)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.Cantidad_Hijos)
        </td>
        <td>
          <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
          <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
          <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
        </td>
      </tr>
      }
    </tbody>
  </table>
</SistemaWebMic.ViewModels.PersonaViewModels>